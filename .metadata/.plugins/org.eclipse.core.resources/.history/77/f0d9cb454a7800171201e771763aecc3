package pierwszapaczka;

public class TestOperatory {

	public static void main(String[] args) {
		int a = 17;
		int b = 4;
		int c = a+b;
		System.out.println(c);
		c = a-b;
		System.out.println(c);
		c = a*b;
		System.out.println(c);
		c = a/b;
		System.out.println(c);
		a/=b;
		System.out.println(a);
		a+=b;
		System.out.println(a);
		a-=b;
		System.out.println(a);
		a*=b;
		System.out.println(a);
		System.out.println(a++);
		System.out.println(++a);
		System.out.println(a--);
		System.out.println(--a);
	}

}
