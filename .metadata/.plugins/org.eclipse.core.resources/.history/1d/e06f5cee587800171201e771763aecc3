package dzien1;

public class J16 {

	public static void main(String[] args) {
		int x = 10 ;
		int y = 5 ;
		boolean result;
		result = x>y;
		System.out.println(result);
		result = 2*x>y ;
		System.out.println(result);
		result = y<x+3 && y>x-2;
		System.out.println(result);
		int z=(x*y)%2;
		result = z==0;
		System.out.println(result);
		

	}

}
