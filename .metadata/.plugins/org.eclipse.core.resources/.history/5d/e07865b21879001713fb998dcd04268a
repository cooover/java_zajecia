package mathtests;

public class testIfElse {

	public static void main(String[] args) {
		int a = 10;
		int b = 100;
		int c = 50;
		if(a>b) {
			if(a>c) {
				System.out.println("A > B oraz A > C");
			}
			else {
				System.out.println("A > B ale A<C");
			}
		}
		else if(a==b){
			System.out.println("A==B");
		}
		else {
			System.out.println("A < B oraz A != B");
		}

	}

}
