package dzien2;
import static java.lang.Math.*;
public class J18 {

	public static void main(String[] args) {
		double a = 3.0;
		double b = -5.0;
		double r = 5.5;
		System.out.println(Math.sqrt(a));//pierwiastek z 3
		System.out.println(Math.abs(b));//wartosc bezwzgledna z -5
		System.out.println(Math.pow(a,b));//3 do potegi -5
		System.out.println(Math.pow(a,Math.abs(b)));//3 do potegi 5
		System.out.println(Math.sqrt(Math.abs(b)));// pierwiastek z 5
		System.out.println(round(Math.PI* Math.pow(r, 2)));//pole kola
		System.out.println(2*Math.PI*r);//dlugosc okregu
	}

}
